msgid "PayDo"
msgstr "PayDo"

# Настройка приема электронных платежей через PayDo.
msgid "Take payments via PayDo"
msgstr "Take payments via PayDo"

# Шлюз отключен
msgid "Gateway is disabled"
msgstr "Gateway is disabled"

# PayDo не поддерживает валюты Вашего магазина.
msgid "PayDo does not support the currency of your store"
msgstr "PayDo does not support the currency of your store"

# Включить платежи через PayDo
msgid "Enable PayDo payments"
msgstr "Enable PayDo payments"

# Включить/Отключить
msgid "Enable/Disable"
msgstr "Enable/Disable"

# Название способа оплаты
msgid "Name of payment gateway"
msgstr "Name of payment gateway"

# Название способа оплаты, которое увидит пользователь при оформлении заказа
msgid ""
"The name of the payment gateway that the user see when placing the order"
msgstr ""
"The name of the payment gateway that the user see when placing the order"

# Public key
msgid "Public key"
msgstr "Public key"

# Секретный ключ
msgid "Secret key"
msgstr "Secret key"

# Выдается в Личном кабинете https://paydo.com
msgid "Issued in the client panel https://paydo.com"
msgstr "Issued in the client panel https://paydo.com"

# Описание способа оплаты, которое клиент будет видеть на вашем сайте.
msgid ""
"Description of the payment gateway that the client will see on your site"
msgstr ""
"Description of the payment gateway that the client will see on your site"

# Автозавершение заказа
msgid "Order completion"
msgstr "Order completion"

# Автоматический перевод заказа в статус \"Выполнен\" после успешной оплаты
msgid ""
"Automatic transfer of the order to the status \"Completed\" after successful "
"payment"
msgstr ""
"Automatic transfer of the order to the status \"Completed\" after successful "
"payment"

# Язык платежной формы
msgid "Payment form language"
msgstr "Payment form language"

# Выберите язык платежной формы для Вашего магазина
msgid "Select the language of the payment form for your store"
msgstr "Select the language of the payment form for your store"

# Русский
msgid "Russian"
msgstr "Russian"

# Английский
msgid "English"
msgstr "English"

# Оплата заказа #
msgid "Payment order #"
msgstr "Payment order #"

# Запрос к платежному сервису был отправлен некорректно
msgid "Request to payment service was sent incorrectly"
msgstr "Request to payment service was sent incorrectly"

# Оплатить
msgid "Pay"
msgstr "Pay"

# Отказаться от оплаты & вернуться в корзину
msgid "Refuse payment & return to cart"
msgstr "Refuse payment & return to cart"

# Спасибо за Ваш заказ, пожалуйста, нажмите кнопку ниже, чтобы заплатить.
msgid "Thank you for your order, please click the button below to pay"
msgstr "Thank you for your order, please click the button below to pay"

# Платеж успешно оплачен
msgid "Payment successfully paid"
msgstr "Payment successfully paid"

# Платеж не оплачен
msgid "Payment not paid"
msgstr "Payment not paid"

# Платеж успешно завершен
msgid "Payment completed successfully"
msgstr "Payment completed successfully"
